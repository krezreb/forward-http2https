[Unit]
Description=Forwards all incoming http connections on port 8086 to https:443
After=docker.service
           
[Service]
Type=simple
WorkingDirectory=/usr/local/bin/forwardhttp2https
ExecStart=/usr/local/bin/forwardhttp2https/docker-compose up
ExecStop=/usr/local/bin/forwardhttp2https/docker-compose down
;Environment=SSH_AUTH_SOCK=%tdd/keyring/ssh
;Restart=always
           
[Install]
WantedBy=default.target
